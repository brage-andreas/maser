{
	"name": "maser",
	"version": "2.0.0",
	"description": "A Discord bot made with discord.js",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"preclearCmds": "npm run build",
		"prebuildCmds": "npm run build",
		"clearCmds": "node scripts/commands/clear.js",
		"buildCmds": "node scripts/commands/build.js",
		"lint:fix": "eslint src --ext .ts --fix",
		"prebuild": "npx rimraf dist",
		"prestart": "npm run build",
		"format": "prettier --write \"**/*.+(js|ts|json)\"",
		"build": "tsc",
		"start": "node .",
		"lint": "eslint src --ext .ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hellobafu/maser.git"
	},
	"engines": {
		"node": ">= 17.1.0",
		"npm": ">= 8.1.3"
	},
	"author": "Drango",
	"license": "MIT",
	"homepage": "https://github.com/hellobafu/maser#readme",
	"dependencies": {
		"@discordjs/rest": "^0.4.1",
		"discord.js": "^14.0.0-dev.1651493371-4ba0f56",
		"dotenv": "^16.0.0",
		"ms": "^2.1.3",
		"pg-promise": "^10.11.1"
	},
	"devDependencies": {
		"@types/ms": "^0.7.31",
		"@typescript-eslint/eslint-plugin": "^5.22.0",
		"@typescript-eslint/parser": "^5.22.0",
		"discord-api-types": "^0.32.1",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.6.2",
		"typescript": "^4.6.4"
	}
}
